	<!-- 查询条件定义 -->
	<sql id="query_where">
		<if test="entity!=null">
			<if test="entity.salaryId != null and entity.salaryId!='' ">
        		and salary_id=#{entity.salaryId}
      		</if>
			<if test="entity.id != null and entity.id!='' ">
